{
  "name": "vuexam",
  "version": "0.0.1",
  "main": "dist/main/index.cjs",
  "author": "Thomas Michael Weissel",
  "license": "GPL",
  "description": "An exam managment tool for digital exams",
  "type": "module",
  "scripts": {
    "vite": "vite ./packages/renderer",
    "dev": "vite ./packages/renderer | npm run serve  ",
    "dev:electron": "node scripts/watch.mjs",
    "build:electron": "node scripts/build.mjs && electron-builder",
    "serve": "nodemon packages/server/src/server"
  },
  "build": {
    "productName": "VuExam",
    "appId": "com.vuexam.app",
    "asar": true,
    "directories": {
      "output": "release/${version}"
    },
    "files": [
      "dist"
    ],
    "linux": {
      "target": "AppImage",
      "category": "Utility",
      "icon": "packages/renderer/public/favicon.svg"
    },
    "mac": {
      "artifactName": "${productName}_${version}.${ext}",
      "target": [
        "dmg"
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],
      "artifactName": "${productName}_${version}.${ext}"
    },
    "nsis": {
      "oneClick": false,
      "perMachine": false,
      "allowToChangeInstallationDirectory": true,
      "deleteAppDataOnUninstall": false
    }
  },
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.14",
    "@vitejs/plugin-vue": "^2.1.0",
    "electron": "16.0.8",
    "electron-builder": "^22.14.5",
    "nodemon": "^2.0.15",
    "typescript": "^4.5.5",
    "vite": "^2.7.13",
    "vite-plugin-resolve": "^1.6.1",
    "vue": "^3.2.31"
  },
  "env": {
    "//": "Used in build scripts",
    "PORT": 3344
  },
  "keywords": [
    "vite",
    "electron",
    "vue3",
    "rollup"
  ],
  "dependencies": {
    "@tiptap/extension-bold": "^2.0.0-beta.26",
    "@tiptap/extension-bullet-list": "^2.0.0-beta.26",
    "@tiptap/extension-code": "^2.0.0-beta.26",
    "@tiptap/extension-code-block-lowlight": "^2.0.0-beta.68",
    "@tiptap/extension-document": "^2.0.0-beta.15",
    "@tiptap/extension-dropcursor": "^2.0.0-beta.25",
    "@tiptap/extension-gapcursor": "^2.0.0-beta.34",
    "@tiptap/extension-hard-break": "^2.0.0-beta.30",
    "@tiptap/extension-heading": "^2.0.0-beta.26",
    "@tiptap/extension-history": "^2.0.0-beta.21",
    "@tiptap/extension-horizontal-rule": "^2.0.0-beta.31",
    "@tiptap/extension-italic": "^2.0.0-beta.26",
    "@tiptap/extension-list-item": "^2.0.0-beta.20",
    "@tiptap/extension-ordered-list": "^2.0.0-beta.27",
    "@tiptap/extension-paragraph": "^2.0.0-beta.23",
    "@tiptap/extension-strike": "^2.0.0-beta.27",
    "@tiptap/extension-subscript": "^2.0.0-beta.10",
    "@tiptap/extension-superscript": "^2.0.0-beta.10",
    "@tiptap/extension-text": "^2.0.0-beta.15",
    "@tiptap/extension-text-align": "^2.0.0-beta.29",
    "@tiptap/extension-underline": "^2.0.0-beta.23",
    "@tiptap/starter-kit": "^2.0.0-beta.183",
    "@tiptap/vue-3": "^2.0.0-beta.90",
    "@types/screenshot-desktop": "^1.12.0",
    "archiver": "^5.3.0",
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-fileupload": "^1.3.1",
    "html2canvas": "^1.4.1",
    "ip": "^1.1.5",
    "jquery": "^3.6.0",
    "jspdf": "^2.5.1",
    "node-notifier": "^10.0.1",
    "puppeteer": "^13.4.1",
    "sass": "^1.49.9",
    "screenshot-desktop": "^1.12.7",
    "vue-router": "^4.0.13"
  }
}
